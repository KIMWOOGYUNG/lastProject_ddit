@Controller
public class MsgController {

	@GetMapping("/note/entirenote") //전체쪽지
	public String Entirenote() {
		String goPage="Entirenote.note";
		return goPage;
		
	}
	@GetMapping("/note/receivenote") //수신쪽지
	public String Receivenote() {
